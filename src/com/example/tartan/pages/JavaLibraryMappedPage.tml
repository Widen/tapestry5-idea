<t:border pageTitle="Java Library Test"
          xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
          xmlns:p="tapestry:parameter">

	<h1>Plaid!</h1>

    <t:if test="thisOrThat">

        <p>${thisText}</p>

        <p:else>
            <p>${thatText}</p>
        </p:else>

    </t:if>

	<t:randomplaid alt="literal:Random plaid image" foo="literal:bar" width="168" height="168" />

</t:border>
